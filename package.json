{
  "name": "palmo-trans-calculator-backend",
  "version": "1.0.0",
  "description": "Node.js/Express backend for PALMO-TRANS delivery calculator",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node -P tsconfig.json src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "watch": "tsc --watch",
    "dev:watch": "nodemon --exec ts-node src/server.ts"
  },
  "keywords": [
    "delivery",
    "calculator",
    "paypal"
  ],
  "author": "PALMO-TRANS",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.13.5",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "nodemailer": "^8.0.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.2",
    "@types/nodemailer": "^6.4.14",
    "nodemon": "^3.1.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
